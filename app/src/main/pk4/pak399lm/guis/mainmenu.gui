	windowDef Squishy {
		rect		0,0,0,0
		visible		0
		notime		1
		
		onTime 0 {
			set	"cmd"	"play guisounds_idlogo";
			set "idlogo::rect" "0 0 640 480";
			resetCinematics;
			set "desktop::doomme" "0";
