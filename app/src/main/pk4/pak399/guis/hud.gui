//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Radio/Audio log window on/off
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	onNamedEvent radioChatterUp {
		set "RadioWin::visible" "1" ;
		set "DDWaveForm::visible" "1" ;
		set "RadioWin2Scroll1::visible" "1" ;
		set "RadioWin2Scroll2::visible" "1" ;
		if ("AirWin::visible" == 0) { 
			transition "RadioWin::rect" "135 435 0 64" "135 435 128 64" "200" ;
		} else {
			transition "RadioWin::rect" "146 400 0 64" "146 400 128 64" "200" ;
		}
	}
	onNamedEvent radioChatterDown {
		set "RadioWin::visible" "0" ;
		set "DDWaveForm::visible" "0" ;
		set "RadioWin2Scroll1::visible" "0" ;
		set "RadioWin2Scroll2::visible" "0" ;
	}
	
	onNamedEvent audioLogUp {
		set "audioWin::visible" "1" ;
		set "audiowin::rect" "-18 402 248 64" ;
		set "audioWaveForm::visible" "1" ;
		set "audioWin2Scroll1::visible" "1" ;
		set "audioWin2Scroll2::visible" "1" ;
	}
	onNamedEvent audioLogDown {
		set "audioWin::visible" "0" ;
		set "audiowin::rect" "-18 402 100 64" ;
		set "audioWaveForm::visible" "0" ;
		set "audioWin2Scroll1::visible" "0" ;
		set "audioWin2Scroll2::visible" "0" ;
	}

