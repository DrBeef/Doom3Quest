//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Tip Window on/off
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

	onNamedEvent tipLeftWindowUp {
		set "TipPopBox::visible" "1" ;
		resetTime "tipWindow" "0" ;		
	}

	onNamedEvent tipWindowUp {
		set "TipPopBox::visible" "1" ;
		resetTime "tipWindow" "0" ;		
	}

	onNamedEvent tipWindowDown {
		set "TipPopBox::visible" "0" ;
