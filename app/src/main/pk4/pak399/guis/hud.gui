//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Soul Cube ready pulse
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++	

	onNamedEvent soulCubeReady {
		set "CubePulseParent::setvis" "1" ;
		resetTime "CubePulseParent" "0" ;
	}
	onNamedEvent soulCubeNotReady {
		set "CubePulseParent::setvis" "0" ;
		set "CubePulse::visible" "0" ;
	}


//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Clip Low / Clip Empty Changes
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	onNamedEvent updateAmmo {
		if ("weaphud" == 0) {
			if ("gui::player_clip_low" == 1) {
				set "Lines2A::matcolor" "1 0.5 0.2 0.2" ;
				set "ammovalue2::forecolor" "1 0.6 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "1" ;
				set "BorderLA_2::visible" "0" ;
			} else {
				set "Lines2A::matcolor" "0.3 0.8 0.7 0.2" ;
				set "ammovalue2::forecolor" "0.67 0.7 0.7 1" ;
				set "BorderLA::visible" "1" ;
				set "BorderLA_1::visible" "0" ;
				set "BorderLA_2::visible" "0" ;
			}
		
			if ("gui::player_clip_empty" == 1) {
				set "Lines2A::matcolor" "1 0 0 0.2" ;
				set "ammovalue2::forecolor" "1 0 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "0" ;
				set "BorderLA_2::visible" "1" ;
			}
		
			if ("gui::player_ammo_empty" == 1) {
				set "Lines1A::matcolor" "1 0 0 0.2" ;
				set "Lines2A::matcolor" "1 0 0 0.2" ;
				set "ammovalue2::forecolor" "1 0 0 1" ;
				set "ammovalue3::forecolor" "1 0 0 1" ;
				set "BorderLA::visible" "0" ;
				set "BorderLA_1::visible" "1" ;
			} else {
				set "Lines1A::matcolor" "0.3 0.8 0.7 0.2" ;
				set "ammovalue3::forecolor" "0.35 0.42 0.4 1" ;
			}
		}
	}
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Objective Window Popup Trigger
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	onNamedEvent newObjective {
		resetTime "AnimNewUp" "0" ; 
		set "objdone2" "1" ;
	}

	onNamedEvent newObjectiveComplete {
		resetTime "AnimComUp" "0" ; 
		set "objdone2" "1" ;
	}
	
	onNamedEvent closeObjective {
		if ("objdone2" == 1) { 
			resetTime "AnimObjDown" "0" ; 
			set "objdone2" "0" ;
		}
	}

//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
//  Air Indicator
//++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

	onNamedEvent noAir {
		if ( "AirWin::visible" == 0 ) {
			localSound "guisounds_air" ;
			set "AirWin::visible" "1" ;
			transition "AirWin::rect" "135 435 0 64" "135 435 128 64" "200" ;
			transition "RadioWin::rect" "135 435 128 64" "146 400 128 64" "100" ;
		}
	}

	onNamedEvent Air {
		if ( "AirWin::visible" != 1 ) {
			set "AirWin::visible" "0" ;
			transition "RadioWin::rect" "146 400 128 64" "135 435 128 64" "100" ;
		}
	}

