	windowDef animPDA {
		rect	0 ,0 ,0 ,0 
		notime	1

		onTime 350 {			
			transition 	"PDABox5::bordercolor" "0.14 0.39 0.38 0" "0.14 0.39 0.38 0.15" "250" ;
		}
		onTime 400 {			
			transition 	"PDABox4::bordercolor" "0.14 0.39 0.38 0" "0.14 0.39 0.38 0.3" "250" ;
		}
		onTime 450 {			
			transition 	"PDABox3::bordercolor" "0.14 0.39 0.38 0" "0.14 0.39 0.38 0.5" "250" ;
		}		
		onTime 500 {
			set		"PDABox2::rect" "250 360 0 10" ;
			set		"PDABox1::rect" "250 360 140 10" ;
			set		"PDABox2::backcolor" "0.4 0.6 0.7 0.25" ;
			transition	"PDABox1::bordercolor" "0.4 0.6 0.6 0" "0.4 0.6 0.6 1" "250" ;
		}

		onTime 600 {
			transition	"PDABox2::rect" "250 360 0 10" "250 360 140 10" "2000" ;
		}
	
		onTime 750 {
			transition	"PDAName1::forecolor" "1 1 1 0.6" "0.6 0.8 0.8 1" "250" ;
			transition	"PDAName1a::forecolor" "0 0 0 0" "0 0 0 0.6" "250" ;			
		}
		onTime 1000 {
			transition	"PDAName2::forecolor" "1 1 1 0.8" "0.4 0.6 0.6 1" "250" ;
			transition	"PDAName2a::forecolor" "0 0 0 0" "0 0 0 0.6" "250" ;	
		}
		onTime 2500 {
			transition	"PDABox1::bordercolor" "1 1 1 0.7" "0.4 0.6 0.6 1" "250" ;
			transition	"PDABox2::backcolor" "0.6 0.7 0.8 0" "0.4 0.6 0.7 0.25" "200" ;
			transition	"PDABox3::bordercolor" "1 1 1 0.4" "0.14 0.39 0.38 0.5" "300" ;
			transition "PDANameDone::forecolor" "0.8 1 1 0.8" "0.8 1 1 0" "400" ;
		
		}
		onTime 2600 {
			transition	"PDAName3::forecolor" "1 1 1 0" "0.8 0.6 0.2 1" "250" ;
			transition	"PDAName3a::forecolor" "0 0 0 0" "0 0 0 0.6" "250" ;	
		}
		
		onTime 2900 {
			transition 	"PDABox3::bordercolor" "0.14 0.39 0.38 0.5"  "0.14 0.39 0.38 0""50" ;
			transition 	"PDABox4::bordercolor" "0.14 0.39 0.38 0.3"  "0.14 0.39 0.38 0""50" ;
			transition 	"PDABox5::bordercolor" "0.14 0.39 0.38 0.15"  "0.14 0.39 0.38 0""50" ;
			if ("NewVidVis::visible" == 1) { 
				transition	"PDABox1::rect" "250 360 140 10" "55 360 5 10" "400" "0.2" "0.4" ;
				transition	"PDABox2::rect" "250 360 140 10" "55 360 5 10" "400" "0.2" "0.4" ;			
			} else {
				transition	"PDABox1::rect" "250 360 140 10" "23 360 5 10" "400" "0.2" "0.4" ;
				transition	"PDABox2::rect" "250 360 140 10" "23 360 5 10" "400" "0.2" "0.4" ;
			}
		}		
		onTime 3300 {
			if ("NewVidVis::visible" == 1) { 
				transition	"PDABox1::rect" "55 360 5 10" "53 425 5 10" "200" "0.2" "0.6" ;
				transition	"PDABox2::rect" "55 360 5 10" "53 425 5 10" "200" "0.2" "0.6" ;
				set "NewPDAVis::rect" "30 400 250 80" ;	
				set "NewVidVis::rect" "0 400 250 80" ;
			} else {
				transition	"PDABox1::rect" "23 360 5 10" "23 425 5 10" "200" "0.2" "0.6" ;
				transition	"PDABox2::rect" "23 360 5 10" "23 425 5 10" "200" "0.2" "0.6" ;
				set "NewPDAVis::rect" "0 400 250 80" ;
				set "NewVidVis::rect" "30 400 250 80" ;
			}
			transition	"PDABox2::backcolor" "0.4 0.6 0.7 0.25" "0.4 0.6 0.7 0" "150" ;
			transition	"PDABox1::bordercolor" "0.4 0.6 0.6 1" "0.4 0.6 0.6 0" "150" ;			
		}
		onTime 3500 {
			set "NEWPDAIcon::visible" "1" ;
		//	set "NewPDA::visible" "1" ;
		}
		onTime 8500 {
			set "NewPDA::visible" "0" ;
			set		"PDABox2::rect" "250 360 0 10" ;
			set		"PDABox1::rect" "250 360 0 10" ;			
		}
		
		
		onTime 3000 {
			transition	"PDAName2::forecolor" "0.4 0.6 0.6 1" "0.4 0.6 0.6 0" "250" ;
			transition	"PDAName1a::forecolor" "0 0 0 0.6" "0 0 0 0" "200" ;	
		}
		onTime 3100 {
			transition	"PDAName1::forecolor" "0.8 0.8 0.8 1" "0.4 0.6 0.6 0" "250" ;
			transition	"PDAName2a::forecolor" "0 0 0 0.6" "0 0 0 0" "200" ;	
		}
		onTime 4600 {
			transition	"PDAName3::forecolor" "0.8 0.6 0.2 0.7" "0.8 0.6 0.2 0" "250" ;
			transition	"PDAName3a::forecolor" "0 0 0 0.6" "0 0 0 0" "200" ;
		}
	}
	
